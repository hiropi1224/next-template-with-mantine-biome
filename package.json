{
  "name": "my-log",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:hcm": "hcm './**/*.{css,scss,sass}' -w --logLevel silent",
    "build": "next build",
    "start": "next start",
    "check:lint": "biome lint .",
    "check:format": "biome format .",
    "check": "biome check .",
    "lint:style": "stylelint './**/*.{css,scss,sass}'",
    "lint:style:fix": "stylelint --fix './**/*.{css,scss,sass}'",
    "fix": "biome check --apply .",
    "generate": "plop"
  },
  "dependencies": {
    "@mantine/core": "^7.10.2",
    "@mantine/hooks": "^7.10.2",
    "next": "14.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "happy-css-modules": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "plop": "^4.0.1",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "sass": "^1.77.5",
    "stylelint": "^16.6.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5"
  }
}
